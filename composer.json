{
    "name": "sausin/laravel-ovh",
    "description": "OVH Object Storage driver for laravel",
    "keywords": [
        "filesystem",
        "filesystems",
        "files",
        "storage",
        "flysystem",
        "openstack",
        "opencloud",
        "swift",
        "ovh",
        "laravel",
        "driver"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Saurabh Singhvi",
            "email": "saurabh.singhvi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sausin\\LaravelOvh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sausin\\LaravelOvh\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "nimbusoft/flysystem-openstack-swift": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10|^10.5|^11.5.3",
        "mockery/mockery": "^1.4.4",
        "friendsofphp/php-cs-fixer": "^3.14",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sausin\\LaravelOvh\\OVHServiceProvider"
            ]
        },
        "hooks": {
            "pre-commit": [
                "composer lint",
                "git update-index --again"
            ],
            "pre-push": [
                "composer test"
            ],
            "post-merge": [
                "composer install"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Sausin\\LaravelOvh\\Composer\\Scripts::devOnly",
            "# cghooks add --ignore-lock"
        ],
        "post-update-cmd": [
            "Sausin\\LaravelOvh\\Composer\\Scripts::devOnly",
            "# cghooks update"
        ],
        "lint": "php-cs-fixer fix",
        "test:lint": "@lint --dry-run",
        "test:unit": "phpunit",
        "test": [
            "@test:lint",
            "@test:unit"
        ]
    }
}
